
public class PolynomialGrapherTester3 extends PolynomialGrapher {
	private static final double[] coef = {0,1,0,0.333333333333,0,0.1333333333333,0,0.053968254,0,0.0218694885,0,0.00886323553};
	private static final Coordinate start = new Coordinate(-2,-8);
	@Override
	public double[] coefficients() {
		return coef;
	}

	@Override
	public double xIncrement() {
		return 0.1;
	}

	@Override
	public Coordinate xyStart() {
		return start;
	}

	@Override
	public double xRange() {
		return 4;
	}

	@Override
	public double yRange() {
		return 16;
	}

	public static void main(String[] args) {
		XYGrapher xyg = new PolynomialGrapherTester3();
		xyg.drawGraph(0, 0, 200, 200);
	}
}
